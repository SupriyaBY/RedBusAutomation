<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Demo" verbose="4"
	thread-count="4" parallel="tests">
	<parameter name="env" value="qa"></parameter>
	<listeners>
		<listener class-name="com.sm.RedBus.listeners.ExtentReportListener" />
		<listener class-name="com.sm.RedBus.listeners.TestAllureListener" />
		<listener class-name="com.sm.RedBus.listeners.AnnotationTransformer" />
	</listeners>
	<test name="HomePageTest">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="115.0" />
		<classes>
			<class name="com.sm.RedBus.tests.HomePageTest" />
		</classes>
	</test>
</suite> <!-- Demo -->
